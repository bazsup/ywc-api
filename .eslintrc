{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "prettier",
    "prettier/standard"
  ],
  "plugins": ["prettier", "standard"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "semi": false,
        "useTabs": false,
        "parser": "babylon",
        "jsxBracketSameLine": true
      }
    ],
    "eqeqeq": ["error", "always"],
    "space-before-function-paren": 0,
    "generator-star-spacing": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "import/no-unresolved": "off",
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2,
    "import/first": false,
    "import/no-named-default": false,
    "import/no-webpack-loader-syntax": false
  }
}